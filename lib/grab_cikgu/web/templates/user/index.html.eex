<h1>Listing Users</h1>

<table class="table">
	<%= for user <- @users do %>
		<tr>
			<td><br><%= render "user.html", user: user %></br></td>
			<td><br><%= link "View", to: user_path(@conn, :show, user.id) %></br></td>
			<td><br><%= link "Profile", to: profile_path(@conn, :show, user.id) %></br></td>
		</tr>
	<% end %>
</table>